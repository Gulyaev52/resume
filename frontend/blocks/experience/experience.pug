include ../list-item/list-item

mixin experience({ company, date, job }) 
    -const optionsListItem = { 
    -    name: company, 
    -    date: date, 
    -    subname: job,
    -    mainClass: "experience", 
    -    nameClass: "experience__company"
    -}
    +list-item(optionsListItem)
        block